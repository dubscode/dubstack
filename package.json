{
  "name": "dubstack-starter",
  "version": "0.1.0",
  "private": true,
  "description": "Dubstack is a reusable Next.js starter.",
  "scripts": {
    "dev": "next dev --turbopack",
    "checks": "biome check --write --unsafe && tsc --noEmit",
    "build": "next build",
    "env:dev": "vercel env pull .env",
    "env:preview": "vercel env pull .env.preview --environment=preview",
    "env:prod": "vercel env pull .env.production --environment=production",
    "start": "next start",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "update:next": "pnpm -r update next@latest react@latest react-dom@latest",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts"
  },
  "packageManager": "pnpm@10.19.0",
  "dependencies": {
    "@clerk/nextjs": "^6.34.0",
    "@clerk/themes": "^2.4.29",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "lucide-react": "^0.546.0",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "posthog-js": "^1.276.0",
    "posthog-node": "^5.10.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.0.4",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "jsdom": "^25.0.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^3.0.4"
  }
}
